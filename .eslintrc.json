{
    "extends": "next/core-web-vitals",
    "no-unused-vars": [
        "error",
        {
            "varsIgnorePattern": "React"
        }
    ],
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "args": "after-used",
                "caughtErrors": "all",
                "ignoreRestSiblings": false,
                "reportUsedIgnorePattern": false
            }
        ]
    }
}
